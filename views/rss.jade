if(rss && rss.channel && rss.channel.item)
	ul.newstab
		- for (var i = 0; i < Math.floor(rss.channel.item.length/2); ++i)	{
			li.newsarea
				- var item = rss.channel.item[i]
				span= '[' + item.source + ']'
				&nbsp;
				a(target='_blank' href= item.link title= item.title)= item.title
				span.pub-date= item.pubDate.replace(/T/,' ').replace(/.000Z/g,'')
		-}
	
